<body id = body style="background-color:red" onkeypress="{
	if(event.keyCode==97){    document.getElementById('my01').style.left  = parseInt(document.getElementById('my01').style.left)-5+'px' }   // small 'a' key go left
	if(event.keyCode==119){   document.getElementById('my01').style.top  = parseInt(document.getElementById('my01').style.top)-5+'px'}   // small 'w' key go up
    if(event.keyCode==100){    document.getElementById('my01').style.left  = parseInt(document.getElementById('my01').style.left)+5+'px'}   // small 'd' key go right
    if(event.keyCode==115){   document.getElementById('my01').style.top  = parseInt(document.getElementById('my01').style.top)+5+'px'}     // small 's' key go dow
}">




